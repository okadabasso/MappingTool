{
  "NLog": {
    "internalLogLevel": "Info",
    "internalLogFile": "c:\\temp\\internal-nlog.txt",
    "extensions": [
      {
        "assembly": "NLog.Extensions.Logging"
      }
    ],
    "targets": {
      "logfile": {
        "type": "File",
        "fileName": "logs/file.txt",
        "layout": "${longdate} ${uppercase:${level}} ${message} ${exception}"
      },
      "logconsole": {
        "type": "Console"
      }
    },
    "rules": [
      {
        "logger": "*",
        "minLevel": "Trace",
        "writeTo": "logconsole"
      },
      {
        "logger": "*",
        "minLevel": "Debug",
        "writeTo": "logfile"
      }
    ]
  }
}
